{
	"method": "get",
	"uri": "/api/user",
	"query": {
		"table": "users",
		"orderBy": "id",
		"descending": false,
		"pagination": {
			"page": "parseInt(query.page)",
			"limit": "parseInt(query.limit)"
		}
	},
	"response": {
		"state": {
			"no_results": {
				"contentType": "application/json",
				"code": 404,
				"template": "pageApi.no_results.tmpl"
			},
			"success": {
				"contentType": "application/json",
				"code": 200,
				"template": "pageApi.success.tmpl"
			}
		}
	}
}
